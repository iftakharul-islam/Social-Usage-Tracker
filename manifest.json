{
    "manifest_version": 3,
    "name": "Social Media Usage Tracker",
    "version": "1.0",
    "description": "Tracks time spent on social media sites and blocks them when usage exceeds the limit.",
    "permissions": [
      "tabs",
      "storage",
      "notifications",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.jpeg",
        "32": "icons/icon32.jpeg",
        "48": "icons/icon48.jpeg",
        "128": "icons/icon128.jpeg"
      }
      
    },
    "icons": {
        "16": "icons/icon16.jpeg",
        "32": "icons/icon32.jpeg",
        "48": "icons/icon48.jpeg",
        "128": "icons/icon128.jpeg"
    },
    "host_permissions": [
      "*://*.facebook.com/*",
      "*://*.twitter.com/*",
      "*://*.linkedin.com/*",
      "*://*.instagram.com/*",
      "*://*.youtube.com/*"
    ]
  }
  